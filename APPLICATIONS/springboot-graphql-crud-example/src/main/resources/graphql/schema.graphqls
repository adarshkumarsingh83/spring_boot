type Employee {
    id: ID!
    firstName: String
    lastName: String
    career: String
}

type Query {
    getAllEmployee: [Employee]!
    getEmployee(id: ID): Employee
}

input EmployeeBean {
    id: ID!
    firstName: String!
    lastName: String!
    career: String
}

type Mutation {
    saveEmployee(employeeBean:EmployeeBean!): Employee
    updateEmployee(employeeBean:EmployeeBean!): Employee
    removeEmployee(id: ID): Employee
}